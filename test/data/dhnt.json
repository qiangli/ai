{
    "blob": {
        "base": "https://ai.dhnt.io",
        "api_key": "dhnt"
    },
    "assets": [
        {
            "type": "file",
            "base": "$HOME/workspace/ai/swarm/resource/incubator"
        },
        {
            "type": "file",
            "base": "~/workspace/ai/swarm/resource/standard"
        }
    ],
    "roots": {
        "workspace": {
            "name": "Workspace",
            "description": "Primary development workspace",
            "path": "/tmp/workspace"
        },
        "cwd":            {
            "name": "Current Directory",
            "description": "Current working directory",
            "path": "~/workspace/ai"
        },
        "temp":             {
            "name": "Temporary Files",
            "description": "Directory for temporary files",
            "path": "/tmp"
        },
        "dirs": [
            {
                "name": "Incubator Resources",
                "description": "Resource directory for incubating agents, tools, and models",
                "path": "~/workspace/ai/swarm/resource/incubator"
            },
            {
                "name": "Standard Resources",
                "description": "Resource directory for standard agents, tools, and models",
                "path": "~/workspace/ai/swarm/resource/standard"
            }
        ]
    }
}