		{
			"name": "TestGetByVector",
			"type": "go",
			"request": "launch",
			"mode": "exec",
			"program": "./bin/test",
			"args": ["-test.run", "^TestGetByVector$"],
		},
		{
			"name": "AI Dry Run",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/ai/",
			"env": {
				"AI_MODEL": "gpt4o-mini",
				"AI_API_KEY": "sk-1234",
				"AI_BASE_URL": "http://localhost:4000",
				"AI_DEBUG": "true",
			},
			"args": [
				"--dry-run",
				"--verbose",
				"/",
				"what",
				"is",
				"cooking"
			],
			"showLog": false,
			"stopOnEntry": false,
		},
		{
			"name": "Auto select agent",
