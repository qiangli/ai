	"version": "0.2.0",
	"configurations": [
		// go test -c -o bin/test  ./internal/db
		{
			"name": "TestGetByVector",
			"type": "go",
			"request": "launch",
			"mode": "exec",
			"program": "./bin/test",
			"args": ["-test.run", "^TestGetByVector$"],
		},
		{
			"name": "AI Dry Run",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/ai/",
			"env": {
				"AI_MODEL": "gpt4o-mini",
				"AI_API_KEY": "sk-1234",
				"AI_BASE_URL": "http://localhost:4000",
				"AI_DEBUG": "true",
			},
			"args": [
				"--dry-run",
				"--verbose",
				"/",
				"what",
				"is",
				"cooking"
			],
			"showLog": false,
			"stopOnEntry": false,
		},
		{
			"name": "Ask",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/ai/",
			"env": {
				"AI_MODEL": "gpt4o-mini",
				"AI_API_KEY": "sk-1234",
				"AI_BASE_URL": "http://localhost:4000",
				"AI_DEBUG": "true",
			},
			"args": [
				"--verbose",
				"@ask",
				"--message",
				"what is fish"
			],
			"showLog": false,
			"stopOnEntry": false
		},
		{
			"name": "Script",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/ai/",
			"env": {
				"AI_MODEL": "gpt4o-mini",
				"AI_API_KEY": "sk-1234",
				"AI_BASE_URL": "http://localhost:4000",
				"AI_DEBUG": "true",
			},
			"args": [
				"--verbose",
				"/rg",
				"--message",
				"find all dogs and cats in the current directory"
			],
			"showLog": false,
			"stopOnEntry": false
		},
		{
			"name": "Eval",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/ai/",
			"env": {
				"AI_MODEL": "gpt4o-mini",
				"AI_API_KEY": "sk-1234",
				"AI_BASE_URL": "http://localhost:4000",
				"AI_DEBUG": "true",
			},
			"args": [
				"--verbose",
				"@eval",
				"--role", "assistant",
				"--role-prompt", "you are a math assistant",
				"--message",
				"what is the sum of 1+1"
			],
			"showLog": false,
			"stopOnEntry": false
		},
		{
			"name": "SQL",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/ai/",
			"env": {
				"AI_MODEL": "gpt4o-mini",
				"AI_API_KEY": "sk-1234",
				"AI_BASE_URL": "http://localhost:4000",
				"AI_DEBUG": "true",
			},
			"args": [
				"--verbose",
				"@sql",
				"--message",
				"git me all tables in metastore"
			],
			"showLog": false,
			"stopOnEntry": false
		},
		{
			"name": "Doc",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/ai/",
			"env": {
				"AI_MODEL": "gpt4o-mini",
				"AI_API_KEY": "sk-1234",
				"AI_BASE_URL": "http://localhost:4000",
				"AI_DEBUG": "true",
			},
			"args": [
				"--verbose",
				"@doc",
				"--doc-template", "/tmp/work/template.md",
				"--file", "/tmp/work/sample.txt",
				"--message",
				"please help write per the template",
			],
			"showLog": false,
			"stopOnEntry": false
		},
		{
			"name": "Docker",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/cmd/ai/",
			"env": {
				"AI_MODEL": "gpt4o-mini",
				"AI_API_KEY": "sk-1234",
				"AI_BASE_URL": "http://localhost:4000",
				"AI_DEBUG": "true",
			},
			"args": [
				"--verbose",
				"@oh",
				"--message",
				"write simple python and save the code in /tmp/work"
			],
			"showLog": false,
			"stopOnEntry": false
		}
	]
}